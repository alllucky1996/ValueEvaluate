@using Entities.Enums;
@model IEnumerable<Entities.Models.SystemManage.AuditLog>
@{
    string CName = "TimKiem";
}
<table class="table table-hover table-bordered table-striped" id="datatable_none_ajax_source">
        <tr>
            <th>#</th>
            <th>Account Id</th>
            <th>EventDateDetail</th>
            <th>EventType</th>
            <th>OriginalValue</th>
            <th>NewValue</th>
        </tr>
        @if(Model != null && Model.Any())
        {
            int i = 0;
            foreach (var item in Model)
            {
                i++;
              <tr>
                  <td>@i</td>
                  <td>
                      @item.AccountId
                  </td>
                  <td>
                      @item.EventDateDetail
                  </td>
                  <td>
                     @(item.EventType=="C"? "Tạo mới": item.EventType=="U"?"Cập nhật": item.EventType == "D"?"Xóa":"") @item.TableName
                  </td>
                  <td>
                     @item.OriginalValue
                  </td>
                  <td>
                      @item.NewValue
                  </td>
                </tr>
            }
        }
</table>
